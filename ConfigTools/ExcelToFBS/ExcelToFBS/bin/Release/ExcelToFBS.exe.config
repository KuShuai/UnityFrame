<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
      <section name="Paths" type="System.Configuration.DictionarySectionHandler"/>
    </configSections>

    <Paths>
      <add key="WorkDir" value="../../../../../"/>
      <add key="NameSpace" value="AutoGenConfig"/>
      <add key="Excel" value="{WorkDir}Configs/Excel"/>
      <add key="FBSClient" value="{WorkDir}Configs/FBS/Client"/>
      <add key="FBSServer" value="{WorkDir}Configs/FBS/DCServer"/>
      <add key="GenClientCode" value="{WorkDir}Configs/Code/Client/"/>
      <add key="GenServerCode" value="{WorkDir}Configs/Code/Server/"/>
      <add key="ClientType" value="{WorkDir}Client/Assets/Scripts/AutoGenerateMessage/Message.cs"/>
      <add key="ClientCode" value="{WorkDir}Client/Assets/Scripts/AutoGenerateConfigs/"/>
      <add key="ClientData" value="{WorkDir}Client/Assets/PrefabResources/Configs/"/>
      <add key="ServerCode" value="{WorkDir}DCServer/src/dc-server/utils/auto_fbs_config/"/>
      <add key="ServerData" value="{WorkDir}Configs/Excel_Bytes/"/>
      <add key="FlatC" value="{WorkDir}ProgramTools/Executable/flatc.exe"/>
      <add key="CompileRef" value="{WorkDir}ProgramTools/CompileRef/"/>
    </Paths>
  
    <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
